/* CSS para el formulario */

.contenedor-formulario{
    border-radius: 5px;
    background-color: #f2f2f2;
    padding: 20px;
    width: 50%;
    /* display: flex; */
    justify-content: center;
    margin-left: 25%;
    margin-right: 25%;
    border-radius: 7px;
}

.contenedor-formulario h3{
    text-align: center;
    margin-top: 0%;
}

.titulo-formulario{
    font-size: 1.6em;
    margin-bottom: 0%;
}

.flex-container-1{
    display: flex;
}

.flex-container-1 > div{
    /* margin: 0.4rem; */
    margin-right: 2.9rem;
}

.guide-text {
    color: red;
    font-size: 0.76em;
    margin-top: -5px;
    text-align: center;
}

.guide-text-terminos {
    color: red;
    font-size: 0.76em;
    text-align: center;
    margin-top: 1px;
}

.guide-text-terminos-link{
    color: red;
    /* font-size: 0.76rem; */
    margin-top: 0px;
}

.text-center-terminos{
    text-align: center;
}

#body-formulario{
    background-image: url("/img/mudanza.jpg");
    background-size: cover;
    background-position-x: center;
    position: relative;
}

input[type=text],input[type=email],
input[type=password]{
    width: 92%;
    border-radius: 7px;
    padding: 12px 20px;
    margin: 8px 0;
    display: inline-block;
    border: 1px solid #ccc;
    /* border-radius: 4px; */
}

select, input[type=tel] {
    width: 100%;
    border-radius: 7px;
    padding: 12px 20px;
    margin: 8px 0;
    display: inline-block;
    border: 1px solid #ccc;  
}

input[type=submit]{
    width: 100%;
    height: 50px;
    line-height: 50px;
    padding-top: 0;
    padding-bottom: 0;
    font-size: 1.6em;
    background: #259b24;
    color: #fff;
    border: none;
    border-radius: 7px;
    cursor: pointer;
}

input[type=submit]:hover{
    color: lightgray;
    background-color: #0a8f08;
    border-color: #547a29;
    border-radius: 7px;
}


/* FIN CSS para el formulario */

/* CSS Terminos y condiciones */
.text-center-terminos{
    text-align: center;
}

.terminos-body{
    display: block;
    margin: 8px;
}

.terminos-body div{
    display: block;
}

.terminos-body div h2{
    display: block;
    font-size: 1.5em;
    margin-top: 0.83em;
    margin-bottom: 0.83em;
    margin-left: 0;
    margin-right: 0;
    font-weight: bold;
}

.terminos-body div p{
    display: block;
    margin-top: 1em;
    margin-bottom: 1em;
    margin-left: 0;
    margin-right: 0;
}

.terminos-body div ol{
    display: block;
    list-style-type: decimal;
    margin-top: 1em;
    margin-bottom: 1em;
    margin-left: 0;
    margin-right: 0;
    padding-left: 40px;
}

.terminos-body div ol li{
    display: list-item;
}


/* FIN CSS Terminos y condiciones */

/* CSS de Show Data */
.showdata .center{
    text-align: center;
}
.showdata .align-left{
    text-align: left;
}

.showdata h4{
    text-align: center;
}

.showdata table, th, td {
    border: 1px solid;
    margin-left: auto;
    margin-right: auto;
}

.showdata img {        
    display:block;
    margin-left:auto;
    margin-right:auto;
}

.showdata .fieldset-auto-width {
    display: inline-block;
}
/* Fin de Show Data */

