#subnav {
    position: sticky;
    top: 60px;
    display: flex;
}

.subnav_container {
    background: #fff;
    align-items: baseline;
    display: flex;
    /* margin: 0 60px; */
    padding: 15px 0;
    padding-left: 200px;
    width: 100%;
    border-bottom: 1px solid #222;
    /* padding-right: 17px; */
}

.subnav_list {
    margin: 0 15px;
}

.subnav_item {
    background: #fff;
    display: inline-block;
    margin: 0 8px;
    padding: 5px;
}

.subnav_title {
    padding-left: 160px;
    padding-right: 30px;
}

