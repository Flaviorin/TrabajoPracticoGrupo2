    font-size: 15px;
}

@import url('https://fonts.googleapis.com/css2?family=Roboto&display=swap');

.nav {
	position: fixed;
	display: flex;
	justify-content: space-between;
	align-items: center;
	height: 60px;
    width: 100%;
	top: 0;
    background: #42A5F5;
    z-index: 9;
}

#imagen_home_logo_camion{
    width: 232px;
}

.nav_item, .subnav_item {
    display: inline-block;
	margin: 0px 10px;
}

.nav_item_stilo, .sub_nav_item_stilo {
    color: black;
    text-decoration: none;
    font-size: 18px;
    font-weight: bold;
}

.presentacion {
	display: grid;
	justify-content: center;
	background: #BBDEFB;
    margin-top: 40px;
    width: 101%;
